<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
		<Nullable>enable</Nullable>
		
        <ContentTargetFolders>.</ContentTargetFolders>
        <Product>Qualia.Umb.Controllers</Product>
        <PackageId>Qualia.Umb.Controllers</PackageId>
        <Title>Qualia.Umb.Controllers</Title>
		<Description>
			An umbraco cms package with abstract controllers.
		</Description>
		<PackageProjectUrl>https://github.com/PagonisTasos/Qualia.Umb.Controllers</PackageProjectUrl>
        <PackageTags>umbraco plugin package; controller</PackageTags>
        <RootNamespace>Qualia.Umb.Controllers</RootNamespace>
		<Version>1.1.0</Version>
		<Authors>Pagonis Tasos</Authors>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Umbraco.Cms.Web.Website" Version="9.1.2" />
        <PackageReference Include="Umbraco.Cms.Web.BackOffice" Version="9.1.2" />
    </ItemGroup>
</Project>
